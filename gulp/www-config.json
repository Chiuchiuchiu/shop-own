{

  "distPath":"apps/www/assets/dist/",
  "buildPath":"apps/www/assets/build/",
  "ts":{
    "watch":"apps/www/assets/src/ts/**/*.ts",
    "src":[
      "apps/www/assets/src/ts/**/*.ts",
      "!apps/www/assets/src/ts/**/*.d.ts"
    ],
    "path":"js/"
  },
  "scss":{
    "watch":"apps/www/assets/src/scss/**/*.scss",
    "src":[
      "apps/www/assets/src/scss/**/*.scss",
      "!apps/www/assets/src/scss/**/*.d.scss"
    ],
    "path":"css/"
  },
  "scripts":{
    "src":[
      "apps/www/assets/src/lib/**/*.js"
    ],
    "path":"js/lib"
  },
  "image":{
    "watch":"apps/www/assets/src/img/**/*.+(jpeg|jpg|png|gif)",
    "src":"apps/www/assets/src/img/**/*.+(jpeg|jpg|png|gif)",
    "path":"img/"
  },
  "scriptOrder":[
    "apps/www/assets/src/js/lib/jquery-3.1.1.min.js",
    "apps/www/assets/src/js/lib/require.js",
    "apps/www/assets/src/js/lib/keyboard.js",
    "apps/www/assets/src/js/lib/LocalResizeIMG.js",
    "apps/www/assets/src/ts/widget/**/*.ts",
    "apps/www/assets/src/ts/app.ts"
  ],
  "concatJs":{
    "src":[
      "apps/www/assets/build/js/lib/jquery-3.1.1.min.js",
      "apps/www/assets/build/js/lib/require.js",
      "apps/www/assets/build/js/lib/keyboard.js",
      "apps/www/assets/build/js/lib/LocalResizeIMG.js",
      "apps/www/assets/build/js/script/app_all.js"
    ],
    "dist":"js/"
  }
}